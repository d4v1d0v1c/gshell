%abp_msp_analog_power_up
rfr 0x00071824
wtr 0x00071848 0x00000002
wtr 0x00071848 0x00008000

%route Vol1 and Vol2 to headphones
rfr 0x00071824
wtr 0x00071848 0x00000041
wtr 0x00071848 0x00006362

%route Vol3 and Vol4 to Main Out
rfr 0x00071824
wtr 0x00071848 0x0000003f
wtr 0x00071848 0x00006465

%route Vol3 and Vol4 to SPDIF Out
rfr 0x00071824
wtr 0x00071848 0x0000000d
wtr 0x00071848 0x00006465

%do some initialization for SPDIF Out
%Route DTV spdif out to SPDIF Out pin
rfr 0x00071824
wtr 0x00071848 0x0000800c
wtr 0x00071848 0x00000090
rfr 0x00071824
wtr 0x00071848 0x00000003
wtr 0x00071848 0x00000356
rfr 0x00071824
wtr 0x00071848 0x00008003
wtr 0x00071848 0x00000000

%do some initialization for I2S Out
rfr 0x00071824
wtr 0x00071848 0x0000001e
wtr 0x00071848 0x0000a440
rfr 0x00071824
wtr 0x00071848 0x0000001f
wtr 0x00071848 0x00000100
  
%route Vol3 and Vol4 to I2S Out
rfr 0x00071824
wtr 0x00071848 0x00000020
wtr 0x00071848 0x00006465

%Vol1 set
rfr 0x00071824
wtr 0x00071848 0x00000052
wtr 0x00071848 0

%Vol2 set
rfr 0x00071824
wtr 0x00071848 0x00000053
wtr 0x00071848 0

%Vol3 set
rfr 0x00071824
wtr 0x00071848 0x00000054
wtr 0x00071848 0

%Vol4 set
rfr 0x00071824
wtr 0x00071848 0x00000055
wtr 0x00071848 0

%Master Vol set
rfr 0x00071824
wtr 0x00071848 0x00000050
wtr 0x00071848 0

%Enable SPDIF1 in, source SPDIF_IN
rfr 0x00071824
wtr 0x00071848 0x00000004
wtr 0x00071848 0x00008000


